{
  "name": "a-level-chem-interactive",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "prebuild": "npm run sync-renderer",
    "build": "tsc -b && vite build",
    "pretest": "npm run sync-renderer",
    "preview": "vite preview",
    "lint": "eslint .",
    "test": "node tests/paths.test.js && node tests/structure.test.js && node tests/visual-smoke.test.js && node tests/m2-animation-standard.test.js && node tests/m2-animation-visuals.test.js && bash scripts/test-edge.sh",
    "test:edge": "bash scripts/test-edge.sh",
    "test:visual": "npm run build && node scripts/visual-smoke.js",
    "test:visual:fast": "node scripts/visual-smoke.js",
    "sync-renderer": "node scripts/sync-mechanism-renderer.mjs",
    "verify:local": "node tests/paths.test.js && npm run build && bash scripts/test-edge.sh"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.85.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "playwright": "^1.58.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.9.2",
    "vite": "^7.1.3"
  }
}
